{
    "data_path": "/workspace/quant_code_dataset", 
    "num_years_train": 3,
    "start_year": 2017,
    "end_year": 2023,
    
    "min_samples": 1650,
    "min_trading_volume": 5000000,
    "feature_threshold": 0.75,
    "min_price": 2,

    "lgbm_params": {
        "max_depth": -1,
        "objective": "regression",
        "num_leaves": 31,
        "learning_rate": 0.1,
        "verbose": -1,
        "min_child_samples": 30,
        "n_estimators": 10000,
        "subsample": 0.7,
        "colsample_bytree": 0.7,
        "early_stopping_rounds": 100,
        "log_evaluation_freq": 500
    },
    
    "num_workers": 40,
    "num_simulations": 3,
    "device_type": "gpu"
}
